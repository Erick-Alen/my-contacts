{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
			// "plugin:prettier/recommended"
	],
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"plugins": [
			"react"
			// "prettier"
	],
	"rules": {
		"no-unused-vars": "off",
		// "prettier/prettier": "error",
		"indent": [
				"warn",
				"tab"
		],
		"linebreak-style": [
				"warn",
				"unix"
		],
		"quotes": [
				"warn",
				"single"
		],
		"semi": [
				"warn",
				"always"
		]
	}
}
